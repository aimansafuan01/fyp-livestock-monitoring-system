    <!------------------------------- Top Navbar ----------------------->


    <!-------------------- GET CURRENT DATE ---------------------->
    <div class="row mt-4 m-4">
      <% 
        for (const data of allCoop) {
          if (data.coopID === 'SB') {
            continue;
          }
      %>
      <div class="col-12 col-md-4 mb-5">
        <div class="card enlargeable">
          <div class="card-header p-3 pt-2">
            <div class="icon-lg shadow-dark text-center border-radius-xl mt-n4 position-absolute bg-gray-200"
              style="display: flex; align-items: center; justify-content: center;">
              <h3 class="text-black m-0"><%= data.coopID %></h3>
            </div>
            <div class="text-end pt-1">
              <p class="text-sm mb-0 text-capitalize">Ratio</p>
              <% const ratio = +data.numOfHens / +data.numOfRoosters %>
              <h4 class="mb-0">
                1 : <%= ratio.toFixed(0) %>
              </h4>
            </div>
          </div>
          <hr class="dark horizontal my-0">
          <div class="card-footer p-3">
            <p class="mb-0"><span class="text-capita;ize text-md font-weight-bolder">
                <%= data.numOfRoosters %>
              </span>Roosters</p>
            <p class="mb-0"><span class="text-capita;ize text-md font-weight-bolder">
                <%= data.numOfHens %>
              </span>Hens</p>
            <p class="mb-0"><span class="text-capita;ize text-md font-weight-bolder">
                <%= data.totalChickens %>
              </span>Total</p>
          </div>
          <div class="row mx-3">
            <a class="btn btn-warning" href="/chicken/create?id=<%= data.coopID %>" role="button">Transfer</a>
          </div>
        </div>
      </div>
      <% } %>

