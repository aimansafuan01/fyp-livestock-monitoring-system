    <!-- CREATE ARRAY OF INDIVIDUAL COOP USING COOPID AS ITS KEY
          Eg: { 'BR1': { mortalityRate: value1, numChick: value2 }
          To Access: brooder[index]['brooderID']  -->
    <% 
        const brooder=allBrooder.map(obj=> ({ [obj.brooderID]: obj }));     
    %>

    <!-- CALCULATE AVG MORTALITY FOR ALL COOP -->
    <% 
        const avgMortality=(+brooder[0]['BR1'].mortalityRate + +brooder[1]['BR2'].mortalityRate + +brooder[2]['BR3'].mortalityRate
        + +brooder[3]['BR4'].mortalityRate + +brooder[4]['BR5'].mortalityRate + +brooder[5]['BR6'].mortalityRate) / 9;
        
        avgMortality === 0 ? avgMortality : avgMortality.toFixed(1)
    %>

      <!-- CALCULATE CHICK AGE -->
    <%  
        const calcAge = (index, id) => {
          let currDate = new Date();
          let insertedAt = new Date(brooder[index][id].inserted_at);
          let timeDiff = currDate - insertedAt;
          let age = Math.floor(timeDiff / (1000 * 60 * 60 * 24))
          return age === 0 ? 1 : age;
        }
    %>
        <!-- TOTAL CALCULATION FOR ALL BROODER -->
    <%
            let totalChick = 0;
            let totalChickAvailable = 0;
            let totalChickBlocked = 0;
            brooder.forEach(brooderObject => {
              const [brooderID, brooderData] = Object.entries(brooderObject)[0];
              totalChick += brooderData.numChick;
              totalChickAvailable += brooderData.availableChick;
              totalChickBlocked += brooderData.blockedChick;
            })
    %>


