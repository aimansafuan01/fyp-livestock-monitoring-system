    <script>
      let counter = 2;

      function addNewSection(sectionId) {
        const sectionContainer = document.getElementById('formRepeat');

        // Create a new div for each instance of Section 1
        const newSection = document.createElement('div');
        newSection.id = `section1_${counter}`;

        // Add the content of Section 1 (you can customize this content)
        newSection.innerHTML = `
            <div class="row border px-2 py-2 mx-1">
            <div class="row mx-1">
              Chicken Mortality
            </div>
            <div class="row w-25 mt-1">
              <div class="col">
                <select class="form-select form-select-sm" name="coopID">
                  <option selected>Coop ID</option>
                  <option>R1</option>
                  <option>R2</option>
                  <option>R3</option>
                  <option>R4</option>
                  <option>R5</option>
                  <option>R6</option>
                  <option>R7</option>
                  <option>R8</option>
                  <option>R9</option>
                </select>
              </div>
            </div>
            <div class="row">
              <div class="col pb-2">
                <label for="numOfDeadHensR${counter}" class="form-label pt-2 mb-0">Number of Dead Hens</label>
                <input type="number" class="form-control p-2" name="numOfDeadHensR${counter}" placeholder="Enter number of dead Hens" value="0">
              </div>
              <div class="col pb-2">
                <label for="numOfDeadRoostersR${counter}" class="form-label pt-2 mb-0">Number of Dead Roosters</label>
                <input type="number" class="form-control p-2" name="numOfDeadRoostersR${counter}" placeholder="Enter number of dead Roosters" value="0">
              </div>
            </div>
          </div>
          <div class="d-flex align-item-center m-2">
            <div class="col"></div>
            <span class="material-symbols-outlined" onclick="addNewSection('formRepeat')" style="cursor: pointer;">
              add
            </span>
            <i onclick="addNewSection('formRepeat')" style="cursor: pointer;">Add Record</i>
          </div>
          `;

        // Append the new section to the container
        sectionContainer.appendChild(newSection);

        // Increment the counter for unique IDs
        counter++;
      }
    </script>

