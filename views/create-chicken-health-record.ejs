      <div class="p-3 mx-5 border border-secondary rounded shadow-lg mb-4">
        <form class="needs-validation" action="/submit-chicken-health-record" method="post" role="form"
          class="text-start">
          <div id="formRepeat">
            <div class="row border px-2 py-2 mx-1">
              <div class="row mt-1">
                <div class="col-12 col-md-2">
                  <select class="form-select ps-2" name="origin">
                    <option selected>Choose Origin</option>
                    <% for (coopID of coopIDs) { if (coopID.coopID ==='SB') { continue; } %>
                      <option value="<%= coopID.coopID %>">
                        <%= coopID.coopID %>
                      </option>
                      <% } %>
                  </select>
                </div>
                <div class="col-12 col-md-2">
                  <select class="form-select ps-2" name="symptom">
                    <option selected>Symptom</option>
                    <% for (symptom of healthSymptoms) { %>
                      <option value="<%= symptom.symptom %>">
                        <%= symptom.symptom %>
                      </option>
                      <% } %>
                  </select>
                </div>
                <div class="col-12 col-md-2">
                  <select class="form-select ps-2" name="status">
                    <option selected>Status</option>
                    <% for (status of healthStatus) { %>
                      <option value="<%= status.status %>">
                        <%= status.status %>
                      </option>
                      <% } %>
                  </select>
                </div>
              </div>
              <div class="row mt-1">
                <div class="col-12 col-md-3">
                  <label for="trayPlacement" class="form-label pt-2 mb-0">Number of Hens</label>
                  <input type="number" class="form-control text-black ps-2" name="numOfHens" min="0" value="0">
                </div>
                <div class="col-12 col-md-3">
                  <label for="trayPlacement" class="form-label pt-2 mb-0">Number of Roosters</label>
                  <input type="number" class="form-control text-black ps-2" name="numOfRoosters" min="0" value="0">
                </div>
              </div>
              <div class="col-12 mt-3 align-self-end d-flex">
                <button class="btn btn-warning" type="submit">Submit form</button>
              </div>
            </div>
          </div>
        </form>
      </div>

