<% 
  
  // Button disable for Monday & Thursday
  let insertTrayFlag = "disabled";
  const currDate = new Date(); 
  const currDay = currDate.getDay(); // Return 0 for Sunday, 1 for Monday
  if (currDay === 1 || currDay === 4) {
    insertTrayFlag="";
  }
  
  // Button disable for hatching date
  let inc1_flag = "disabled";
  let inc2_flag = "disabled";
  if (!hatchingDate || hatchingDate.length == 0) {
    console.log("No hatching today")
  } else {
    if (hatchingDate.length == 2) {
      inc1_flag = "";
      inc2_flag = "";
    }
  
    if (hatchingDate[0] && hatchingDate[0].incubatorID == 'INC1') {
      inc1_flag = "";
    }
  
    if (hatchingDate[0] && hatchingDate[0].incubatorID == 'INC2') {
      inc2_flag = "";
    }
  
    if (hatchingDate[1] && hatchingDate[1].incubatorID == 'INC2') {
      inc2_flag = "";
    }
  }
%>

